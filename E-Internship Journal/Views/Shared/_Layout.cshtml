    <script>
        $(document).ready(function () {
            $.fn.dataTable.moment('HH:mm MMM D, YY'); //format sort date
            $.fn.dataTable.moment('dddd, MMMM Do, YYYY'); //format sort date

            $("#submit").click(function () {
                console.log(convertTime12to24($("#timeoutval").val()));
            });
            //------start custom script-----
            $(".navbar-toggler").click(function () {
                $("body").removeClass("sidenav-toggled")
            });
            $("[name='my-checkbox']").bootstrapSwitch();
            //------end custom script-----

            //------start date script-----

            $('#timein').datetimepicker({
                allowInputToggle: true,
                format: 'LT'
            });
            $('#timeout').datetimepicker({
                allowInputToggle: true,
                format: 'LT'
            });
            var date_input = $('input[name="date"]'); //our date input has the name "date"
            var container = $('.bootstrap-iso form').length > 0 ? $('.bootstrap-iso form').parent() : "body";
            date_input.datepicker({
                format: 'dd/mm/yyyy',
                todayHighlight: true,
                autoclose: true

            });
            //------end date script-----
            // check if this day has an event before


            function convertTime12to24(time12h) {
                const [time, modifier] = time12h.split(' ');

                let [hours, minutes] = time.split(':');

                if (hours === '12') {
                    hours = '00';
                }

                if (modifier === 'PM') {
                    hours = parseInt(hours, 10) + 12;
                }

                return hours + ':' + minutes;
            }
        });
    </script>
